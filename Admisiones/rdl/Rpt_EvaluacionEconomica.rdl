  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SourceEE</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@fP_MananaRecreativa">
            <Value>=Parameters!fP_MananaRecreativa.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Asp.Nombres + ' ' + Asp.Apellidos AS Aspirante
	   ,Res.Familiar_TipoDocumento AS TipoDocumento
	   ,Res.Familiar_Identificacion AS Identificacion
	   ,Fam.Familiar_Apellidos AS Apellidos
FROM dbo.Responsable Res
JOIN dbo.Familiar fam ON fam.Familiar_TipoDocumento = Res.Familiar_TipoDocumento 
	AND fam.Familiar_Identificacion = Res.Familiar_Identificacion 
	AND fam.Familiar_Aspirante_Identificacion = Res.Familiar_Aspirante_Identificacion 
	AND fam.Familiar_Aspirante_TipoDocumento = Res.Familiar_Aspirante_TipoDocumento
JOIN dbo.Aspirante Asp ON Asp.TipoDocumento = fam.Familiar_Aspirante_TipoDocumento 
	AND Asp.Identificacion = fam.Familiar_Aspirante_Identificacion
JOIN dbo.Admision Adm ON Adm.Aspirante_TipoDocumento = Asp.TipoDocumento 
	AND Adm.Aspirante_Identificacion = Asp.Identificacion
JOIN dbo.Asistencia Asi ON Asi.Codigo_Admision = Adm.Codigo
JOIN dbo.Programacion Prog ON Prog.Id = Asi.Id_Programacion
JOIN dbo.Evento Eve ON Eve.Id = Prog.Evento_Id
WHERE fam.Parentesco IN ('Padre','Madre')
     AND CONVERT(DATE,Adm.MananaRecreativa) = @fP_MananaRecreativa
GROUP BY Asp.Nombres + ' ' + Asp.Apellidos 
	   ,Res.Familiar_TipoDocumento 
	   ,Res.Familiar_Identificacion 
	   ,Fam.Familiar_Apellidos 
	   ,Res.Responsable_EconomicaAcudiente 
	   ,Fam.Parentesco

UNION

SELECT Asp.Nombres + ' ' + Asp.Apellidos AS Aspirante
	   ,Res.Familiar_TipoDocumento AS TipoDocumento
	   ,Res.Familiar_Identificacion AS Identificacion
	   ,Fam.Familiar_Apellidos AS Apellidos
FROM dbo.Responsable Res
JOIN dbo.Familiar fam ON fam.Familiar_TipoDocumento = Res.Familiar_TipoDocumento 
	AND fam.Familiar_Identificacion = Res.Familiar_Identificacion 
	AND fam.Familiar_Aspirante_Identificacion = Res.Familiar_Aspirante_Identificacion 
	AND fam.Familiar_Aspirante_TipoDocumento = Res.Familiar_Aspirante_TipoDocumento
JOIN dbo.Aspirante Asp ON Asp.TipoDocumento = fam.Familiar_Aspirante_TipoDocumento 
	AND Asp.Identificacion = fam.Familiar_Aspirante_Identificacion
JOIN dbo.Admision Adm ON Adm.Aspirante_TipoDocumento = Asp.TipoDocumento 
	AND Adm.Aspirante_Identificacion = Asp.Identificacion
JOIN dbo.Asistencia Asi ON Asi.Codigo_Admision = Adm.Codigo
JOIN dbo.Programacion Prog ON Prog.Id = Asi.Id_Programacion
JOIN dbo.Evento Eve ON Eve.Id = Prog.Evento_Id
WHERE fam.Parentesco = 'Acudiente' AND Res.Responsable_EconomicaAcudiente = 1
     AND CONVERT(DATE,Adm.MananaRecreativa) = @fP_MananaRecreativa
GROUP BY Asp.Nombres + ' ' + Asp.Apellidos 
	   ,Res.Familiar_TipoDocumento 
	   ,Res.Familiar_Identificacion 
	   ,Fam.Familiar_Apellidos 
	   ,Res.Responsable_EconomicaAcudiente 
	   ,Fam.Parentesco</CommandText>
