  <DataSets>
    <DataSet Name="Asignacioncursos">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select e.codigoestudiante, n.nombre as Nivel, right(c.Nombre,1) as Curso from estudiantecurso ec 
inner join estudiante e on e.numeroidentificacion = ec.NumeroIdentificacionEstudiante and e.tipoidentificacion = ec.TipoIdentificacionEstudiante
inner join curso c on c.idcurso = ec.IdCurso and c.AnioAcademico = 12
inner join nivel n on n.IdNivel = c.IdNivel</CommandText>
      </Query>
      <Fields>
        <Field Name="codigoestudiante">
          <DataField>codigoestudiante</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Nivel">
